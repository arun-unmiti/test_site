{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": false
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "MunichRe": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/MunichRe",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": [
              "src/favicon.ico",
              {
                "glob": "**/*",
                "input": "src/assets/saksham",
                "output": "/assets/saksham"
              },
              {
                "glob": "**/*",
                "input": "src/assets/munichre",
                "output": "/assets/munichre"
              },
              {
                "glob": "**/*",
                "input": "src/assets/images",
                "output": "/assets/images"
              }
            ],
            "styles": [
              "node_modules/ngx-owl-carousel-o/lib/styles/prebuilt-themes/owl.carousel.min.css",
              "node_modules/ngx-owl-carousel-o/lib/styles/prebuilt-themes/owl.theme.green.min.css",
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "node_modules/leaflet/dist/leaflet.css",
              "node_modules/leaflet-draw/dist/leaflet.draw.css",
              "node_modules/leaflet.markercluster/dist/MarkerCluster.css",
              "node_modules/leaflet.markercluster/dist/MarkerCluster.Default.css",
              "node_modules/@fortawesome/fontawesome-free/css/all.min.css",
              "node_modules/primeng/resources/themes/saga-blue/theme.css",
              "node_modules/primeng/resources/primeng.min.css",
              "node_modules/primeicons/primeicons.css",
              "node_modules/sweetalert2/dist/sweetalert2.min.css",
              "src/styles.css"
            ],
            "scripts": [
              "node_modules/@mapbox/leaflet-omnivore/leaflet-omnivore.min.js",
              "node_modules/tokml/tokml.js",
              "node_modules/togeojson/togeojson.js"
            ],
            "sourceMap": true,
            "optimization": false
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "10mb",
                  "maximumError": "20mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "15kb"
                }
              ],
              "assets": [
                "src/favicon.ico",
                {
                  "glob": "**/*",
                  "input": "src/assets/saksham",
                  "output": "/assets/saksham"
                },
                {
                  "glob": "**/*",
                  "input": "src/assets/munichre",
                  "output": "/assets/munichre"
                },
                {
                  "glob": "**/*",
                  "input": "src/assets/images",
                  "output": "/assets/images"
                }
              ]
            },
            "saksham": {
              "tsConfig": "tsconfig.app.json",
              "assets": [
                "src/favicon.ico",
                {
                  "glob": "**/*",
                  "input": "src/assets/saksham",
                  "output": "/assets/saksham"
                }
              ],
              "styles": [
                "node_modules/ngx-owl-carousel-o/lib/styles/prebuilt-themes/owl.carousel.min.css",
                "node_modules/ngx-owl-carousel-o/lib/styles/prebuilt-themes/owl.theme.green.min.css",
                "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
                "node_modules/bootstrap/dist/css/bootstrap.min.css",
                "node_modules/leaflet/dist/leaflet.css",
                "node_modules/leaflet-draw/dist/leaflet.draw.css",
                "node_modules/leaflet.markercluster/dist/MarkerCluster.css",
                "node_modules/leaflet.markercluster/dist/MarkerCluster.Default.css",
                "node_modules/@fortawesome/fontawesome-free/css/all.min.css",
                "node_modules/primeng/resources/themes/saga-blue/theme.css",
                "node_modules/primeng/resources/primeng.min.css",
                "node_modules/primeicons/primeicons.css",
                "node_modules/sweetalert2/dist/sweetalert2.min.css",
                "src/styles.css"
              ],
              "scripts": [
                "node_modules/@mapbox/leaflet-omnivore/leaflet-omnivore.min.js",
                "node_modules/tokml/tokml.js",
                "node_modules/togeojson/togeojson.js"
              ]
            },
            "munichre": {
              "tsConfig": "tsconfig.app.json",
              "assets": [
                "src/favicon.ico",
                {
                  "glob": "**/*",
                  "input": "src/assets/munichre",
                  "output": "/assets/munichre"
                },
                {
                  "glob": "**/*",
                  "input": "src/assets/images",
                  "output": "/assets/images"
                }
              ],
              "styles": [
                "node_modules/ngx-owl-carousel-o/lib/styles/prebuilt-themes/owl.carousel.min.css",
                "node_modules/ngx-owl-carousel-o/lib/styles/prebuilt-themes/owl.theme.green.min.css",
                "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
                "node_modules/bootstrap/dist/css/bootstrap.min.css",
                "node_modules/leaflet/dist/leaflet.css",
                "node_modules/leaflet-draw/dist/leaflet.draw.css",
                "node_modules/leaflet.markercluster/dist/MarkerCluster.css",
                "node_modules/leaflet.markercluster/dist/MarkerCluster.Default.css",
                "node_modules/@fortawesome/fontawesome-free/css/all.min.css",
                "node_modules/primeng/resources/themes/saga-blue/theme.css",
                "node_modules/primeng/resources/primeng.min.css",
                "node_modules/primeicons/primeicons.css",
                "node_modules/sweetalert2/dist/sweetalert2.min.css",
                "src/styles.css"
              ],
              "scripts": [
                "node_modules/@mapbox/leaflet-omnivore/leaflet-omnivore.min.js",
                "node_modules/tokml/tokml.js",
                "node_modules/togeojson/togeojson.js"
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "MunichRe:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "MunichRe:build:production"
            },
            "saksham": {
              "browserTarget": "MunichRe:build:saksham"
            },
            "munichre": {
              "browserTarget": "MunichRe:build:munichre"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "MunichRe:build"
          }
        },
        "test": {
          "builder": "@angular-builders/jest:run",
          "options": {
            "tsConfig": "tsconfig.spec.json",
            "polyfills": ["zone.js", "zone.js/testing"],
            "assets": ["src/favicon.ico"],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "src/styles.css"
            ],
            "runInBand": true,
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "MunichRe:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "MunichRe:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "MunichRe"
}